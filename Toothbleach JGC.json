{
	"name": "Toothbleach JGC",
	"style": "Light",
	"author": {
		"name": "Jonathan Clark"
	},
	"editor": {
		"backgroundColor": "#FAFAFA",
		"altBackgroundColor": "#F0F0F0",
		"toolbarBackgroundColor": "#F0F0F0",
		"tintColor": "#C87230",
		"tintColor2": "#769EB3",
		"textColor": "#222E33",
		"toolbarIconColor": "#222E33",
		"menuItemColor": "#222E33",
		"timeBlockColor": "#E9C062",
		"shouldOverwriteFont": false
	},

	"__orderedStyles": [
		"title-mark1",
		"title-mark2",
		"title-mark3",
		"body",
		"quote-content",
		"bold",
		"bold-left-mark",
		"bold-right-mark",
		"italic",
		"italic-left-mark",
		"italic-right-mark",
		"boldItalic",
		"boldItalic-left-mark",
		"boldItalic-right-mark",
		"code",
		"code-left-backtick",
		"code-right-backtick",
		"special-char",
		"checked-todo-characters",
		"todo",
		"checked",
		"quote-mark",
		"tabbed",
		"NoteLinks-main",
		"link",
		"hashtag",
		"attag",
		"schedule-to-date-link",
		"done-date",
		"schedule-from-date-link",
		"title1",
		"title2",
		"title3",
		"note-title-link"
	],

	"styles": {

		"body": {
			"color": "#222E33",
			"font": "AvenirNext-Regular",
			"lineSpacing": 0.8,
			"paragraphSpacing": 8,
			"paragraphSpacingBefore": 0
		},

		"title1": {
			"color": "#C5487A",
			"size": 24,
			"font": "AvenirNext-Bold"
		},
		"title-in-frontmatter": {
			"regex": "^[tT]itle:\\s+(.*)",
			"matchPosition": 1,
			"color": "#C5487A",
			"size": 22,
			"font": "AvenirNext-Bold"
		},
		"title2": {
			"color": "#AB5699",
			"size": 22,
			"font": "AvenirNext-DemiBold"
		},
		"title3": {
			"color": "#7B72B9",
			"size": 20,
			"font": "AvenirNext-DemiBold"
		},
		"title4": {
			"color": "#8B82C9",
			"size": 18,
		   "font": "AvenirNext-DemiBold"
		},

		"title-mark1": {
			"color": "#66DAE3E8",
			"size": 28,
			"paragraphSpacingBefore": 18,
			"paragraphSpacing": 2,
			"font": "AvenirNext-DemiBold"
		},
		"title-mark2": {
			"color": "#66E9C062",
			"size": 24,
			"paragraphSpacingBefore": 18,
			"paragraphSpacing": 2,
			"font": "AvenirNext-DemiBold"
		},
		"title-mark3": {
			"color": "#66E9C062",
			"font": "AvenirNext-DemiBold",
			"size": 20,
			"paragraphSpacingBefore": 14,
			"paragraphSpacing": 1
		},
		"title-mark4": {
			"color": "#66E9C062",
			"font": "AvenirNext-DemiBold",
			"paragraphSpacingBefore": 14,
			"paragraphSpacing": 1
		},

		"bold": {
			"color": "#885555",
			"color-old": "#996666",
			"font": "AvenirNext-Bold"
		},
		"bold-left-mark": {
			"color": "#CC6666"
		},
		"bold-right-mark": {
			"color": "#CC6666"
		},
		"italic": {
			"color": "#885555",
			"color-old": "#774444",
			"font": "AvenirNext-Italic"
		},
		"italic-left-mark": {
			"color": "#BB8888"
		},
		"italic-right-mark": {
			"color": "#BB8888"
		},
		"boldItalic": {
			"color": "#885555",
			"color-old": "#996666",
			"font": "AvenirNext-BoldItalic"
		},
		"boldItalic-left-mark": {
			"color": "#CC6666"
		},
		"boldItalic-right-mark": {
			"color": "#CC6666"
		},

		"todo": {
			"font": "noteplanstate",
			"color": "#CC6666",
			"headIndent": 33,
      "size": 16,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
		},

		"checked-todo-characters": {
			"font": "noteplanstate",
			"headIndent": 33,
      "size": 16
		},

		"special-char": {
			"font": "Menlo-Regular"
		},

		"tabbed": {
			"headIndent": 43
		},

		"quote-mark": {
			"color": "#35443250",
			"font": "noteplanstate",
			"headIndent": 33,
			"size": 16
		},
		"quote-content": {
			"color": "#434343",
			"type": "italic",
			"headIndent": 33,
			"lineSpacing": 0.90,
			"paragraphSpacing": 4,
			"paragraphSpacingBefore": 0
		},

		"markdown-link": {
			"color": "#C89240"
		},

		"markdown-link-url": {
			"color": "#465E68"
		},

		"link": {
			"color": "#C88E6B"
		},

		"time": {
			"color": "#E36868"
		},

		"schedule-to-date-link": {
			"color": "#E8AE9B"
		},

		"done-date": {
			"color": "#BBBBBB"
		},

		"schedule-from-date-link": {
			"color": "##E8AE9B"
		},

		"note-title-link": {
			"color": "#97B853"
		},

		"hashtag": {
			"_color": "#88482B",
			"color": "#5A64A2",
			"type": ""
		},

		"attag": {
			"_color": "#88482B",
			"color": "#5A64A2",
			"type": ""
		},

		"phonenumber": {
			"color": "#769EB3"
		},

		"field": {
			"regex": "^(\\h*\\-?\\h*)([^:\\+\\*]{3,26})(:)(?=\\h+[\\w\\d@!#]+)",
			"COMMENT": "Simplified form of YAML field identification",
			"color": "#5A64A2",
			"matchPosition": 1
		},
		"field-right-colons": {
			"regex": "^(\\h*\\-?\\h*)([^:\\+\\*]{3,26})(:)(?=\\h+[\\w\\d@!#]+)",
			"color": "#5A64A2",
			"matchPosition": 2,
			"isMarkdownCharacter": true
		},

		"attribute": {
			"regex": "^([^:]{3,})(::)",
			"color": "#5A64A2",
			"matchPosition": 1
		},
		"attribute-right-colons": {
			"regex": "^([^:]{3,})(::)",
			"color": "#5A64A2",
			"matchPosition": 2,
			"isMarkdownCharacter": true
		},

		"highlighted": {
			"regex": "(==)([^\\s].+?)(==)",
			"backgroundColor": "#55D2D21B",
			"order": 35,
			"matchPosition": 2,
			"isRevealOnCursorRange": true
		},
		"highlighted-left-marker": {
			"regex": "(==)([^\\s].+?)(==)",
			"color": "#AA45A2E5",
			"backgroundColor": "#7745A2E5",
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true,
			"matchPosition": 1
		},
		"highlighted-right-marker": {
			"regex": "(==)([^\\s].+?)(==)",
			"color": "#AA45A2E5",
			"backgroundColor": "#7745A2E5",
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true,
			"matchPosition": 3
		},

		"NoteLinks-main": {
			"regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 2,
			"color": "#C5487A"
		},
		"NoteLinks-left-brackets": {
			"regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 1,
			"color": " #C5487A",
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},
		"NoteLinks-right-brackets": {
			"regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 3,
			"color": " #C5487A",
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},

		"NoteLinks-main-done": {
			"regex": "(?:^|\\h)(x\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"isRevealOnCursorRange": true,
			"font": "noteplanstate",
			"color": "#97B853"
		},
		"NoteLinks-main-cancelled": {
			"regex": "(?:^|\\h)(-\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"isRevealOnCursorRange": true,
			"font": "noteplanstate",
			"color": "#C5487A"
		},

		"strikethrough": {
			"regex": "(~~)([^~]+)(~~)",
			"matchPosition": 2,
			"strikethroughStyle": 2,
			"isRevealOnCursorRange": true
		},
		"strikethrough-left-tilde": {
			"regex": "(~~)([^~]+)(~~)",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"color": "#d9d9d9",
			"isRevealOnCursorRange": true
		},
		"strikethrough-right-tilde": {
			"regex": "(~~)([^~]+)(~~)",
			"matchPosition": 3,
			"isMarkdownCharacter": true,
			"color": "#d9d9d9",
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},

		"checked": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\]).*",
			"color": "#A0098308",
			"_color": "#97B863",
			"matchPosition": 2,
			"headIndent": 33,
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"checked-canceled": {
			"regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ).*",
			"matchPosition": 0,
			"color": "#A0E04F57",
			"_color": "#E04F57",
			"strikethroughStyle": 1,
			"headIndent": 33,
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"checked-scheduled": {
			"regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] ).*",
			"matchPosition": 0,
			"color": "#A07B7C86",
			"_color": "#7B7C86",
			"headIndent": 33,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
		},

		"deadline": {
			"regex": "\\h+!(\\d{4}-\\d{2}-\\d{2})\\h*",
			"matchPosition": 1,
			"color": "#AA45A2E5"
		},

		"flagged-1": {
			"regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{1}\\h.*)|(.*\\h+\\!{1}\\h.*)|(.*\\h\\!{1}))$",
			"matchPosition": 2,
			"backgroundColor": "#FFE5E5"
		},
    "flagged-2": {
			"regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{2}\\h.*)|(.*\\h+\\!{2}\\h.*)|(.*\\h\\!{2}))$",
			"matchPosition": 2,
			"backgroundColor": "#FFC5C5"
    },
    "flagged-3": {
			"regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{3}\\h.*)|(.*\\h+\\!{3}\\h.*)|(.*\\h\\!{3}))$",
			"matchPosition": 2,
			"backgroundColor": "#FFA5A5"
    },

		"starred": {
			"regex": "(^|\\v)(.*⭐️.*)($|\\v)",
			"matchPosition": 2,
			"backgroundColor": "#FFE5E5"
		},

		"code": {
			"font": "Menlo-Regular",
			"color": "#4A54A2",
			"backgroundColor": "#F4F4FB",
      "size": 14
		},
		"code-left-backtick": {
			"color": "#804A54A2"
		},
		"code-right-backtick": {
			"color": "#804A54A2"
		},
		"code-fence": {
			"theme": "github",
			"body": {
				"font": "Menlo-Regular",
        "paragraphSpacingBefore": 0,
        "paragraphSpacing": 0,
        "size": 14,
				"backgroundColor": "#e9f9e9"
			},
			"corner-radius": 8,
			"fence-open": {
				"color": "#6584A2"
			},
			"fence-close": {
				"color": "#6584A2"
			},
			"language-specifier": {
				"font": "Menlo-Italic",
				"color": "#afafaf",
        "size": 14,
        "paragraphSpacing": 0
			}
		},
		"#waiting": {
			"regex": "^* (.*#waiting.*)$",
			"matchPosition": 1,
			"textColor": "#222E3388",
			"font": "AvenirNext-Italic"
		},
		"Templates_tags-opener": {
			"regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
			"matchPosition": 1,
			"color": "#6584A2",
			"font": "Menlo-Regular"
		},
		"Templates_tags": {
			"regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
			"matchPosition": 2,
			"color": "#3B3AB2",
			"font": "Menlo-Regular"
		},
		"Templates_tags-closer": {
			"regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
			"matchPosition": 3,
			"color": "#6584A2",
			"font": "Menlo-Regular"
		},
		"Templates_tags-commented-out": {
			"regex": "(<%#\\s+.*?\\s+[\\-]?%>)",
			"matchPosition": 1,
			"color": "#6584A2",
			"font": "AvenirNext-Italic"
		},
		"eventID": {
		  "regex": "(event:[A-F0-9-]{36,37})",
		  "matchPosition": 1,
		  "color": "#CCA949",
		  "isHiddenWithoutCursor": true,
		  "isRevealOnCursorRange": true
		},
		"timeblocks": {
			"regex": "(?:^\\s*(?:\\*(?!\\s+\\[[\\-\\>]\\])\\s+|\\-(?!\\h+\\[[\\-\\>]\\]\\s+)|[\\d+]\\.|\\#{1,5}\\s+))(?:\\[\\s\\]\\s+)?.*?\\s(((at|from)\\s+([0-2]?\\d|noon|midnight)(:[0-5]\\d)?(\\s?(AM|am|PM|pm)?)(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d)(:[0-5]\\d)?(\\s*(AM|am|PM|pm)?))?|([0-2]?\\d|noon|midnight)(:[0-5]\\d)\\s*(AM|am|PM|pm)?(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d|noon|midnight)(:[0-5]\\d)?(\\s*(AM|am|PM|pm)?)?)?))(?=\\s|$)",
		  "matchPosition": 1,
		  "color": "#CC4999"
    },
		"line-comment-hide": {
			"regex": "(.*)( // )(.*)",
			"matchPosition": 3,
			"isHiddenWithoutCursor": true,
			"color": "#AA888888"
		},
		"line-comment-show": {
			"regex": "(.* // .*)",
			"matchPosition": 1,
			"isRevealOnCursorRange": true
		},
		"line-comment-slash": {
			"regex": "(.*)( // )",
			"matchPosition": 2,
			"isMarkdownCharacter": true,
			"color": "#AA888888"
		},
		"unfinished": {
			"regex": "\\?\\?\\?",
			"color": "#FFFFFF",
			"backgroundColor": "#FF2020"
		}
	}
}
