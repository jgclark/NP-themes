{
  "name": "Toothpaste",
  "COMMENT": "This is NOT updated for new theming features in NP 3.9.4 yet, so there's a version that can work with earlier versions.",
  "style": "Dark",
  "author": {
    "name": "Jonathan Clark"
  },
  "editor": {
    "backgroundColor": "#1D1E1F",
    "altBackgroundColor": "#2E2F30",
    "toolbarBackgroundColor": "#2E2F30",
    "tintColor": "#E9C0A2",
    "tintColor2": "#73B3C0",
    "textColor": "#DAE3E8",
    "toolbarIconColor": "#c5c5c0",
    "menuItemColor": "#c5c5c0",
    "timeBlockColor": "#E9C062",
    "shouldOverwriteFont": false
  },
  "__orderedStyles": [
    "title-mark1",
    "title-mark2",
    "title-mark3",
    "body",
    "quote-content",
    "bold",
    "bold-left-mark",
    "bold-right-mark",
    "italic",
    "italic-left-mark",
    "italic-right-mark",
    "boldItalic",
    "boldItalic-left-mark",
    "boldItalic-right-mark",
    "code",
    "code-left-backtick",
    "code-right-backtick",
    "special-char",
    "checked-todo-characters",
    "todo",
    "checked",
    "quote-mark",
    "tabbed",
    "NoteLinks-main",
    "link",
    "hashtag",
    "attag",
    "schedule-to-date-link",
    "done-date",
    "schedule-from-date-link",
    "title1",
    "title2",
    "title3",
    "note-title-link"
  ],
  "styles": {
    "body": {
      "color": "#DAE3E8",
      "font": "AvenirNext-Regular",
      "lineSpacing": 0.8,
      "paragraphSpacing": 8,
      "paragraphSpacingBefore": 0
    },
    "title1": {
      "color": "#B74746",
      "_color": "#CC6666",
      "font": "AvenirNext-Bold",
      "size": 24,
      "paragraphSpacingBefore": 18,
      "paragraphSpacing": 2
    },
    "title-in-frontmatter": {
      "regex": "^[tT]itle:\\s+(.*)",
      "matchPosition": 1,
      "color": "#B74746",
      "_color": "#CC6666",
      "size": 22,
      "font": "AvenirNext-Bold"
    },
    "title2": {
      "color": "#C99056",
      "font": "AvenirNext-DemiBold",
      "size": 22,
      "paragraphSpacingBefore": 18,
      "paragraphSpacing": 2
    },
    "title3": {
      "color": "#E9C062",
      "font": "AvenirNext-DemiBold",
      "size": 20,
      "paragraphSpacingBefore": 14,
      "paragraphSpacing": 1
    },
    "title4": {
      "color": "#E9C062",
      "font": "AvenirNext-DemiBold",
      "size": 18,
      "paragraphSpacingBefore": 14,
      "paragraphSpacing": 1
    },
    "title-mark1": {
      "color": "#66DAE3E8",
      "font": "AvenirNext-DemiBold",
      "size": 28
    },
    "title-mark2": {
      "color": "#66E9C062",
      "font": "AvenirNext-DemiBold",
      "size": 24
    },
    "title-mark3": {
      "color": "#66E9C062",
      "font": "AvenirNext-DemiBold",
      "size": 20
    },
    "title-mark4": {
      "color": "#66E9C062",
      "font": "AvenirNext-DemiBold",
      "size": 16
    },
    "bold": {
      "color": "#B74746",
      "_color": "#CC6666",
      "font": "AvenirNext-Bold"
    },
    "bold-left-mark": {
      "color": "#99CC6666"
    },
    "bold-right-mark": {
      "color": "#99CC6666"
    },
    "italic": {
      "color": "#9B4241",
      "_color": "#CC6666",
      "font": "AvenirNext-Italic"
    },
    "italic-left-mark": {
      "color": "#99CC6666"
    },
    "italic-right-mark": {
      "color": "#99CC6666"
    },
    "boldItalic": {
      "color": "#B74746",
      "_color": "#CC6666",
      "font": "AvenirNext-BoldItalic"
    },
    "boldItalic-left-mark": {
      "color": "#99CC6666"
    },
    "boldItalic-right-mark": {
      "color": "#99CC6666"
    },
    "todo": {
      "font": "noteplanstate",
      "color": "#B74746",
      "_color": "#CC6666",
      "headIndent": 33,
      "size": 16,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "checked-todo-characters": {
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },
    "checked": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\]).*",
      "color": "#D0098308",
      "_color": "#9DC777",
      "matchPosition": 2,
      "headIndent": 33,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "checked-canceled": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ).*",
      "matchPosition": 0,
      "color": "#A0E04F57",
      "_color": "#AD6777",
      "headIndent": 33,
      "strikethroughStyle": 1,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] ).*",
      "matchPosition": 0,
      "color": "#A07B7C86",
      "_color": "#AAAA99",
      "headIndent": 33,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "special-char": {
      "COMMENT": "Overriding to avoid appearance of em-dash for hyphens.",
      "__font": "Menlo-Regular",
      "font": "AvenirNext-Regular"
    },
    "tabbed": {
      "headIndent": 43
    },
    "quote-mark": {
      "color": "#354432",
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },
    "quote-content": {
      "color": "#c5c5c0",
      "type": "italic",
      "headIndent": 33,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4,
      "paragraphSpacingBefore": 0
    },
    "link": {},
    "schedule-to-date-link": {},
    "done-date": {
      "color": "#656560"
    },
    "schedule-from-date-link": {},
    "note-title-link": {},
    "hashtag": {
      "_color": "#E7C1C2",
      "color": "#96CBFE",
      "type": ""
    },
    "attag": {
      "_color": "#E7C1C2",
      "color": "#96CBFE",
      "type": ""
    },
    "phonenumber": {
      "color": "#769EB3"
    },
    "NoteLinks-main": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 2,
      "color": "#C5487A",
      "backgroundColor": "#22C5487A"
    },
    "NoteLinks-left-brackets": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 1,
      "color": " #C5487A",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "NoteLinks-right-brackets": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 3,
      "color": " #C5487A",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "NoteLinks-main-done": {
      "regex": "(?:^|\\h)(x\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true,
      "font": "noteplanstate",
      "color": "#97B853"
    },
    "NoteLinks-main-cancelled": {
      "regex": "(?:^|\\h)(-\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true,
      "font": "noteplanstate",
      "color": "#C5487A"
    },
    "field": {
      "regex": "^(\\h*\\-?\\h*)([^:\\+\\*]{3,26})(:)(?=\\h+[\\w\\d@!#]+)",
      "COMMENT": "Simplified form of YAML field identification",
      "color": "#96CBFE",
      "matchPosition": 1
    },
    "field-right-colons": {
      "regex": "^(\\h*\\-?\\h*)([^:\\+\\*]{3,26})(:)(?=\\h+[\\w\\d@!#]+)",
      "color": "#8B82C9",
      "matchPosition": 2,
      "isMarkdownCharacter": true
    },
    "attribute": {
      "regex": "^([^:]{3,})(::)",
      "color": "#96CBFE",
      "matchPosition": 1,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "attribute-right-colons": {
      "regex": "^([^:]{3,})(::)",
      "color": "#8B82C9",
      "matchPosition": 2,
      "isMarkdownCharacter": true,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "highlighted": {
      "regex": "(==)([^\\s].+?)(==)",
      "backgroundColor": "#66D2D21B",
      "order": 35,
      "matchPosition": 2,
      "isRevealOnCursorRange": true
    },
    "highlighted-left-colon": {
      "regex": "(==)([^\\s].+?)(==)",
      "color": "#AA45A2E5",
      "backgroundColor": "#7745A2E5",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "matchPosition": 1
    },
    "highlighted-right-colon": {
      "regex": "(==)([^\\s].+?)(==)",
      "color": "#AA45A2E5",
      "backgroundColor": "#7745A2E5",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "matchPosition": 3
    },
    "strikethrough": {
      "regex": "(~~)([^~]+)(~~)",
      "matchPosition": 2,
      "strikethroughStyle": 2,
      "isRevealOnCursorRange": true
    },
    "strikethrough-left-tilde": {
      "regex": "(~~)([^~]+)(~~)",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "color": "#d9d9d9",
      "isRevealOnCursorRange": true
    },
    "strikethrough-right-tilde": {
      "regex": "(~~)([^~]+)(~~)",
      "matchPosition": 3,
      "isMarkdownCharacter": true,
      "color": "#d9d9d9",
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "deadline": {
      "regex": "\\h+!(\\d{4}-\\d{2}-\\d{2})\\h*",
      "matchPosition": 1,
      "color": "#AA45A2E5"
    },
    "flagged-1": {
      "regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{1}\\h.*)|(.*\\h+\\!{1}\\h.*)|(.*\\h\\!{1}))$",
      "matchPosition": 2,
      "backgroundColor": "#A0718A"
    },
    "flagged-2": {
      "regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{2}\\h.*)|(.*\\h+\\!{2}\\h.*)|(.*\\h\\!{2}))$",
      "matchPosition": 2,
      "backgroundColor": "#A0516A"
    },
    "flagged-3": {
      "regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{3}\\h.*)|(.*\\h+\\!{3}\\h.*)|(.*\\h\\!{3}))$",
      "matchPosition": 2,
      "backgroundColor": "#A0314A"
    },
    "starred": {
      "regex": "(^|\\v)(.*⭐️.*)($|\\v)",
      "matchPosition": 2,
      "backgroundColor": "#A0314A"
    },
    "code": {
      "font": "Menlo-Regular",
      "color": "#96CBFE",
      "size": 14
    },
    "code-left-backtick": {
      "color": "#8096CBFE"
    },
    "code-right-backtick": {
      "color": "#8096CBFE"
    },
    "code-fence": {
      "theme": "dark",
      "body": {
        "font": "Menlo-Regular",
        "paragraphSpacingBefore": 0,
        "paragraphSpacing": 0,
        "size": 14,
        "_backgroundColor": "#253422"
      },
      "corner-radius": 7,
      "fence-open": {
        "color": "#afafaf"
      },
      "fence-close": {
        "color": "#afafaf"
      },
      "language-specifier": {
        "font": "Menlo-Italic",
        "color": "#afafaf",
        "size": 14,
        "paragraphSpacing": 0
      }
    },
    "#waiting": {
      "regex": "^* (.*#waiting.*)$",
      "matchPosition": 1,
      "textColor": "#DAE3E888",
      "font": "AvenirNext-Italic"
    },
    "Templates_tags-opener": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 1,
      "color": "#6584A2",
      "font": "Menlo-Regular"
    },
    "Templates_tags": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 2,
      "color": "#96CBFE",
      "font": "Menlo-Regular"
    },
    "Templates_tags-closer": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 3,
      "color": "#6584A2",
      "font": "Menlo-Regular"
    },
    "Templates_tags-commented-out": {
      "regex": "(<%#\\s+.*?\\s+[\\-]?%>)",
      "matchPosition": 1,
      "color": "#6584A2",
      "font": "AvenirNext-Italic"
    },
    "eventID": {
      "regex": "(event:[A-F0-9-]{36,37})",
      "matchPosition": 1,
      "color": "#444444",
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "timeblocks": {
      "regex": "(?:^\\s*(?:\\*(?!\\s+\\[[\\-\\>]\\])\\s+|\\-(?!\\h+\\[[\\-\\>]\\]\\s+)|[\\d+]\\.|\\#{1,5}\\s+))(?:\\[\\s\\]\\s+)?.*?\\s((at|from)\\s+([0-2]?\\d|NOON|noon|MIDNIGHT|midnight)(:[0-5]\\d)?(\\s*(AM|am|PM|pm)?)(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d)(:[0-5]\\d)?(\\s*(AM|am|PM|pm)?))?|(([0-2]?\\d|NOON|noon|MIDNIGHT|midnight)(:[0-5]\\d)?\\s*(AM|am|PM|pm)?(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d|NOON|noon|MIDNIGHT|midnight)(:[0-5]\\d)?(\\s*(AM|am|PM|pm))))|(([0-2]?\\d|NOON|noon|MIDNIGHT|midnight):[0-5]\\d\\s*(AM|am|PM|pm)?)(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d|NOON|noon|MIDNIGHT|midnight):[0-5]\\d)?)(?=\\s|$)",
      "matchPosition": 1,
      "color": "#CC4999"
    },
    "line-comment-hide": {
      "regex": "(.*)( // )(.*)",
      "matchPosition": 3,
      "isHiddenWithoutCursor": true,
      "color": "#AA888888"
    },
    "line-comment-show": {
      "regex": "(.* // .*)",
      "matchPosition": 1,
      "isRevealOnCursorRange": true
    },
    "line-comment-slash": {
      "regex": "(.*)( // )",
      "matchPosition": 2,
      "isMarkdownCharacter": true,
      "color": "#AA888888"
    },
    "unfinished": {
      "regex": "\\?\\?\\?",
      "color": "#000000",
      "backgroundColor": "#FF2020"
    }
  }
}