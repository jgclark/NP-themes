{
  "name": "Toothpaste",
  "COMMENT": "This is NOT updated for new theming features in NP 3.9.4 yet, so there's a version that can work with earlier versions.",
  "style": "Dark",
  "author": {
    "name": "Jonathan Clark"
  },
  "editor": {
    "backgroundColor": "#1D1E1F",
    "altBackgroundColor": "#2E2F30",
    "toolbarBackgroundColor": "#2E2F30",
    "tintColor": "#E9C0A2",
    "tintColor2": "#73B3C0",
    "textColor": "#DAE3E8",
    "toolbarIconColor": "#c5c5c0",
    "menuItemColor": "#c5c5c0",
    "timeBlockColor": "#E9C062",
    "shouldOverwriteFont": false
  },
  "orderedStyles": [
    "title-mark1",
    "title-mark2",
    "title-mark3",
    "body",
    "quote-content",
    "bold",
    "bold-left-mark",
    "bold-right-mark",
    "italic",
    "italic-left-mark",
    "italic-right-mark",
    "boldItalic",
    "boldItalic-left-mark",
    "boldItalic-right-mark",
    "code",
    "code-left-backtick",
    "code-right-backtick",
    "special-char",
    "checked-todo-characters",
    "todo",
    "checked",
    "quote-mark",
    "tabbed",
    "NoteLinks-main",
    "link",
    "hashtag",
    "attag",
    "schedule-to-date-link",
    "done-date",
    "schedule-from-date-link",
    "title1",
    "title2",
    "title3",
    "note-title-link"
  ],
  "styles": {
    "body": {
      "color": "#DAE3E8",
      "font": "AvenirNext-Regular",
      "lineSpacing": 0.8,
      "paragraphSpacing": 8,
      "paragraphSpacingBefore": 0
    },
    "tabbed": {
      "COMMENT": "for anything indented",
      "headIndent": 43,
      "lineSpacing": 0.8,
      "paragraphSpacing": 8,
      "paragraphSpacingBefore": 0
    },
    "title1": {
      "color": "#B74746",
      "font": "AvenirNext-Bold",
      "size": 24,
      "paragraphSpacingBefore": 18,
      "paragraphSpacing": 2
    },
    "title-in-frontmatter": {
      "regex": "^[tT]itle:\\s+(.*)",
      "matchPosition": 1,
      "size": 22,
      "color": "#B74746",
      "font": "AvenirNext-Bold"
    },
    "title2": {
      "color": "#C99056",
      "font": "AvenirNext-DemiBold",
      "size": 22,
      "paragraphSpacingBefore": 18,
      "paragraphSpacing": 2
    },
    "title3": {
      "color": "#E9C062",
      "font": "AvenirNext-DemiBold",
      "size": 20,
      "paragraphSpacingBefore": 14,
      "paragraphSpacing": 1
    },
    "title4": {
      "color": "#E9C062",
      "font": "AvenirNext-DemiBold",
      "size": 18,
      "paragraphSpacingBefore": 14,
      "paragraphSpacing": 1
    },
    "title-mark1": {
      "color": "#66DAE3E8",
      "font": "AvenirNext-DemiBold",
      "size": 22
    },
    "title-mark2": {
      "color": "#66E9C062",
      "font": "AvenirNext-DemiBold",
      "size": 18
    },
    "title-mark3": {
      "color": "#66E9C062",
      "font": "AvenirNext-DemiBold",
      "size": 17
    },
    "title-mark4": {
      "color": "#66E9C062",
      "font": "AvenirNext-DemiBold",
      "size": 15
    },
    "bold": {
      "color": "#B74746",
      "font": "AvenirNext-Bold"
    },
    "bold-left-mark": {
      "color": "#99C75756"
    },
    "bold-right-mark": {
      "color": "#99C75756"
    },
    "italic": {
      "color": "#9B4241",
      "font": "AvenirNext-Italic"
    },
    "italic-left-mark": {
      "color": "#99CC6666"
    },
    "italic-right-mark": {
      "color": "#99CC6666"
    },
    "boldItalic": {
      "color": "#B74746",
      "font": "AvenirNext-BoldItalic"
    },
    "boldItalic-left-mark": {
      "color": "#99CC6666"
    },
    "boldItalic-right-mark": {
      "color": "#99CC6666"
    },
    "underline": {
      "underlineStyle": 2,
      "underlineColor": "#B74746"
    },
    "underline-left-tilde": {
      "color": "#88B74746"
    },
    "underline-right-tilde": {
      "color": "#88B74746"
    },
    "checked-todo-characters": {
      "headIndent": 36,
      "_size": 16,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "todo": {
      "COMMENT": "Covers todos and bullets",
      "font": "noteplanstate",
      "color": "#B74746",
      "headIndent": 36,
      "_size": 16,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "checked": {
      "color": "#D0098308",
      "headIndent": 36,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "checked-canceled": {
      "COMMENT": "Covers whole line, colouring it",
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ).*",
      "matchPosition": 0,
      "color": "#A0E04F57",
      "headIndent": 36,
      "strikethroughStyle": 1,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "checked-canceled-text": {
      "COMMENT": "Strikes out just the text",
      "regex": "(?:(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ))(.*)",
      "matchPosition": 3,
      "color": "#A0E04F57",
      "strikethroughStyle": 1,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] ).*",
      "matchPosition": 0,
      "color": "#A07B7C86",
      "headIndent": 36,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "special-char": {
      "COMMENT": "Overriding to avoid appearance of em-dash for hyphens.",
      "__font": "Menlo-Regular",
      "font": "AvenirNext-Regular"
    },

    "quote-mark": {
      "color": "#354432",
      "font": "noteplanstate",
      "headIndent": 36
    },
    "quote-content": {
      "color": "#c5c5c0",
      "type": "italic",
      "headIndent": 36,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4,
      "paragraphSpacingBefore": 0
    },
"link": {
  "color": "#C88E6B"
},
"schedule-to-date-link": {
  "color": "#E8AE9B"
},
    "done-date": {
      "color": "#656560"
    },
    "schedule-from-date-link": {
      "color": "#E8AE9B"
    },
    "note-title-link": {
      "color": "#97B853"
    },
    "hashtag": {
      "color": "#96CBFE",
      "backgroundColor": "#20553322",
      "borderRadius": 4,
      "leftBorderPadding": -1,
      "rightBorderPadding": -1,
      "type": ""
    },
    "attag": {
      "color": "#90C0E0",
      "backgroundColor": "#20553322",
      "borderRadius": 4,
      "leftBorderPadding": -1,
      "rightBorderPadding": -1,
      "type": ""
    },
    "phonenumber": {
      "color": "#769EB3"
    },
    "NoteLinks-main": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 2,
      "color": "#C5487A",
      "backgroundColor": "#22C5487A"
    },
    "NoteLinks-left-brackets": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 1,
      "color": " #C5487A",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "NoteLinks-right-brackets": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 3,
      "color": " #C5487A",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "NoteLinks-main-done": {
      "regex": "(?:^|\\h)(x\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true,
      "font": "noteplanstate",
      "color": "#97B853"
    },
    "NoteLinks-main-cancelled": {
      "regex": "(?:^|\\h)(-\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true,
      "font": "noteplanstate",
      "color": "#C5487A"
    },
    "URL-link": {
      "regex": "\\[[^]]*?\\]\\(.*?\\)",
      "isRevealOnCursorRange": true,
      "comment": "Put cursor anywhere in markdown link and the []() syntax will be revealed. The following 4 do the hiding the rest of the time."
    },
    "URL-link-left-square-bracket": {
      "COMMENT": "Slightly more complex than the others to avoid problem with start-of-line task markers",
      "regex": "(\\[)([^]]*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 1,
      "isHiddenWithoutCursor": true,
      "isMarkdownCharacter": true
    },
    "URL-link-right-square-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 3,
      "isHiddenWithoutCursor": true,
      "isMarkdownCharacter": true
    },
    "URL-link-left-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 4,
      "isHiddenWithoutCursor": true,
      "isMarkdownCharacter": true
    },
    "URL-link-right-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 6,
      "isHiddenWithoutCursor": true,
      "isMarkdownCharacter": true
    },
    "field-key": {
      "COMMENT": "Simplified form of YAML field identification, limited to 25 chars in field name at start of line",
      "regex": "^([^\\h#:\\-\\+\\*][\\s\\w]{3,26})(:\\h+)(?=[\\w\\d@!#]+)",
      "matchPosition": 1,
      "color": "#96CBFE"
    },
    "field-value": {
      "COMMENT": "now turned off",
      "_regex": "^([^\\h#:\\-\\+\\*][\\s\\w]{3,26})(:\\h+)(?=[\\w\\d@!#]+)",
      "_matchPosition": 2,
      "_color": "#8B82C9",
      "_isMarkdownCharacter": true
    },
    "attribute": {
      "regex": "^([^:]{3,})(::)",
      "matchPosition": 1,
      "color": "#96CBFE",
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "attribute-right-colons": {
      "regex": "^([^:]{3,})(::)",
      "matchPosition": 2,
      "color": "#8B82C9",
      "isMarkdownCharacter": true,
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "highlighted": {
      "backgroundColor": "#66D2D21B",
      "order": 35,
      "isRevealOnCursorRange": true
    },
    "highlighted-left-marker": {
      "color": "#AA45A2E5",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "highlighted-right-marker": {
      "color": "#AA45A2E5",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "strikethrough": {
      "strikethroughStyle": 2,
      "isRevealOnCursorRange": true
    },
    "strikethrough-left-tilde": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "color": "#d9d9d9",
      "isRevealOnCursorRange": true
    },
    "strikethrough-right-tilde": {
      "isMarkdownCharacter": true,
      "color": "#d9d9d9",
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "deadline": {
      "regex": "\\h(!\\d{4}-\\d{2}-\\d{2})\\b",
      "matchPosition": 1,
      "font": "AvenirNextCondensed-Bold",
      "color": "#C53119"
    },
    "working-on": {
      "backgroundColor": "#88006600",
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "flagged-1": {
      "__regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{1}\\h.*)|(.*\\h+\\!{1}\\h.*)|(.*\\h\\!{1}))$",
      "__matchPosition": 2,
      "_backgroundColor": "#A0718A",
      "backgroundColor": "#552E2F",
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "flagged-2": {
      "__regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{2}\\h.*)|(.*\\h+\\!{2}\\h.*)|(.*\\h\\!{2}))$",
      "__matchPosition": 2,
      "_backgroundColor": "#A0516A",
      "backgroundColor": "#773E3F",
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "flagged-3": {
      "__regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{3}\\h.*)|(.*\\h+\\!{3}\\h.*)|(.*\\h\\!{3}))$",
      "__matchPosition": 2,
      "_backgroundColor": "#A0314A",
      "backgroundColor": "#A04E4F",
      "lineSpacing": 0.90,
      "paragraphSpacing": 4
    },
    "code": {
      "font": "Menlo-Regular",
      "color": "#96CBFE",
      "backgroundColor": "#322",
      "size": 14
    },
    "code-left-backtick": {
      "color": "#8096CBFE"
    },
    "code-right-backtick": {
      "color": "#8096CBFE"
    },
    "code-fence": {
      "theme": "dark",
      "body": {
        "font": "Menlo-Regular",
        "paragraphSpacingBefore": 0,
        "paragraphSpacing": 0,
        "size": 14
      },
      "corner-radius": 7,
      "fence-open": {
        "color": "#afafaf"
      },
      "fence-close": {
        "color": "#afafaf"
      },
      "language-specifier": {
        "font": "Menlo-Italic",
        "color": "#afafaf",
        "size": 13,
        "paragraphSpacing": 0
      }
    },
    "#waiting": {
      "regex": "^* (.*#waiting.*)$",
      "matchPosition": 1,
      "textColor": "#DAE3E888",
      "font": "AvenirNext-Italic"
    },
    "Templates_tags-opener": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 1,
      "color": "#BB96CBFE",
      "font": "Menlo-Regular",
      "size": 15
    },
    "Templates_tags": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 2,
      "color": "#96CBFE",
      "font": "Menlo-Regular",
      "size": 15
    },
    "Templates_tags-closer": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 3,
      "color": "#BB96CBFE",
      "font": "Menlo-Regular",
      "size": 15
    },
    "Templates_tags-commented-out": {
      "regex": "(<%#\\s+.*?\\s+[\\-]?%>)",
      "matchPosition": 1,
      "color": "#7796CBFE",
      "font": "Menlo-Regular",
      "size": 15
    },
    "eventID": {
      "regex": "(event:[A-F0-9-]{36,37})",
      "matchPosition": 1,
      "color": "#444444",
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "Timeblock-Hide-Tag": {
      "comment": "FIXME:",
      "regex": "\\s#🕑",
      "matchPosition": 0,
      "backgroundColor": "#1D1E1F",
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": true,
      "type": "nolink"
    },
    "end-of-line-comment-hide": {
      "regex": "(.*)( // )(.*)",
      "matchPosition": 3,
      "isHiddenWithoutCursor": true,
      "color": "#AAE5E5EC"
    },
    "end-of-line-comment-show": {
      "regex": "(.* // .*)",
      "matchPosition": 1,
      "isRevealOnCursorRange": true
    },
    "end-of-line-comment-mark": {
      "regex": "(.*)( // )",
      "matchPosition": 2,
      "isMarkdownCharacter": true,
      "color": "#AA888888"
    },
    "inline-comment": {
      "regex": "(%)(%([^%]+%?)+%)(%)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": true,
      "color": "#AAE5E5EC"
    },
    "inline-comment-mark-start": {
      "regex": "[\\s^](%%)[^\\s]",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "color": "#88818475"
    },
    "inline-comment-mark-end": {
      "regex": "\\b(%%)[\\W^]",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "color": "#88818475"
    },
    "unfinished": {
      "regex": "\\?\\?\\?",
      "color": "#000000",
      "backgroundColor": "#FF2020"
    },
    "arrow-link": {
      "regex": "[\\s^]>([^>]*?)<[$\\s\\b\\.\\)\\]\\}]",
      "matchPosition": 1,
      "urlPosition": 1,
      "backgroundColor": "#10FFFFFF",
      "isRevealOnCursorRange": true,
      "type": "noteLink",
      "prefix": "noteplan://x-callback-url/openNote?noteTitle="
    },
    "arrow-left-delimiter": {
      "regex": "(>)(\\S[^<]*)(<)",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "arrow-right-delimiter": {
      "regex": "(>)(\\S[^<]*)(<)",
      "matchPosition": 3,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "verse-indicator": {
      "regex": "(§)(.*)",
      "matchPosition": 1,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "isMarkdownCharacter": true,
      "color": "#99818475"
    },
    "verse-text": {
      "regex": "(¶)(.*)",
      "matchPosition": 2,
      "color": "#F04060",
      "backgroundColor": "#2E2F30",
      "borderRadius": 4
    }
  }
}