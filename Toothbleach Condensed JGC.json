{
	"name": "Toothbleach Condensed JGC",
	"COMMENT": "This is updated for new theming features in NP 3.9.4. It will not work fully on earlier versions of NP.",
	"style": "Light",
	"author": {
		"name": "Jonathan Clark"
	},
	"comment": "we appear to be able to use #AARRGGBB most of the time",
	"editor": {
		"backgroundColor": "#FAFAFA",
		"altBackgroundColor": "#F0F0F0",
		"toolbarBackgroundColor": "#F0F0F0",
		"tintColor": "#C87230",
		"tintColor2": "#769EB3",
		"textColor": "#222E33",
		"toolbarIconColor": "#222E33",
		"menuItemColor": "#222E33",
		"timeBlockColor": "#769EB3",
		"shouldOverwriteFont": false
	},
	"orderedStyles": [
		"title-mark1",
		"title-mark2",
		"title-mark3",
		"body",
		"quote-content",
		"bold",
		"bold-left-mark",
		"bold-right-mark",
		"italic",
		"italic-left-mark",
		"italic-right-mark",
		"boldItalic",
		"boldItalic-left-mark",
		"boldItalic-right-mark",
		"code",
		"code-left-backtick",
		"code-right-backtick",
		"special-char",
		"checked-todo-characters",
		"todo",
		"checked",
		"checked-canceled",
		"checked-canceled-text",
		"checked-scheduled",
		"quote-mark",
		"tabbed",
		"NoteLinks-main",
		"link",
		"Timeblock-Hide-Tag",
		"hashtag",
		"attag",
		"schedule-to-date-link",
		"done-date",
		"schedule-from-date-link",
		"title1",
		"title2",
		"title3",
		"note-title-link"
	],
	"styles": {
		"body": {
			"color": "#222E33",
			"font": "AvenirNextCondensed-Regular",
			"lineSpacing": 0.8,
			"paragraphSpacing": 8,
			"paragraphSpacingBefore": 0
		},
		"tabbed": {
			"COMMENT": "for anything indented",
			"headIndent": 43,
			"lineSpacing": 0.8,
			"paragraphSpacing": 8,
			"paragraphSpacingBefore": 0
		},
		"fmtest": {
			"COMMENT": "attempting whole FM styling - not working",
			"_regex": "\\A---\\n((.*)(\\n.*)?(\\n.*)?(\\n.*)?)\\n---\\n",
			"matchPosition": 1,
			"color": "#88222E33",
			"lineSpacing": 0.8,
			"paragraphSpacing": 0,
			"paragraphSpacingBefore": 0
		},
		"title1": {
			"color": "#C5487A",
			"font": "AvenirNext-Bold",
			"size": 24,
			"paragraphSpacingBefore": 18,
			"paragraphSpacing": 2
		},
		"title-in-frontmatter": {
			"regex": "^[tT]itle:\\s+(.*)",
			"matchPosition": 1,
			"color": "#C5487A",
			"font": "AvenirNext-Bold",
			"size": 22
		},
		"title2": {
			"color": "#AB5699",
			"font": "AvenirNext-DemiBold",
			"size": 22,
			"paragraphSpacingBefore": 18,
			"paragraphSpacing": 2
		},
		"title3": {
			"color": "#7B72B9",
			"font": "AvenirNext-DemiBold",
			"size": 20,
			"paragraphSpacingBefore": 14,
			"paragraphSpacing": 1
		},
		"title4": {
			"color": "#8B82C9",
			"font": "AvenirNext-DemiBold",
			"size": 18
		},
		"title-mark1": {
			"color": "#66DAE3E8",
			"font": "AvenirNext-DemiBold",
			"size": 22,
		},
		"title-mark2": {
			"color": "#66E9C062",
			"font": "AvenirNext-DemiBold",
			"size": 18
		},
		"title-mark3": {
			"color": "#66E9C062",
			"font": "AvenirNext-DemiBold",
			"size": 17
		},
		"title-mark4": {
			"color": "#66E9C062",
			"font": "AvenirNext-DemiBold",
			"size": 15
		},
		"bold": {
			"color": "#885555",
			"font": "AvenirNextCondensed-Bold"
		},
		"bold-left-mark": {
			"color": "#88885555"
		},
		"bold-right-mark": {
			"color": "#88885555"
		},
		"italic": {
			"color": "#885555",
			"font": "AvenirNextCondensed-Italic"
		},
		"italic-left-mark": {
			"color": "#88885555"
		},
		"italic-right-mark": {
			"color": "#88885555"
		},
		"boldItalic": {
			"color": "#885555",
			"font": "AvenirNextCondensed-BoldItalic"
		},
		"boldItalic-left-mark": {
			"color": "#88885555"
		},
		"boldItalic-right-mark": {
			"color": "#88885555"
		},
		"underline": {
			"underlineStyle": 1,
			"underlineColor": "#885555"
		},
		"underline-left-tilde": {
			"color": "#88885555"
		},
		"underline-right-tilde": {
			"color": "#88885555"
		},
		"checked-todo-characters": {
			"font": "noteplanstate",
			"headIndent": 36,
			"_size": 16,
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"todo": {
			"COMMENT": "Covers todos and bullets",
			"font": "noteplanstate",
			"color": "#CC6666",
			"headIndent": 36,
			"_size": 16,
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"checked": {
			"color": "#A0098308",
			"headIndent": 36,
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"checked-canceled": {
			"COMMENT": "Covers whole line, colouring it",
			"regex": "(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ).*",
			"color": "#A0E04F57",
			"headIndent": 36,
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"checked-canceled-text": {
			"COMMENT": "Strikes out just the text",
			"regex": "(?:(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ))(.*)",
			"matchPosition": 3,
			"color": "#A0E04F57",
			"strikethroughStyle": 1,
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"checked-scheduled": {
			"regex": "(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] ).*",
			"matchPosition": 0,
			"color": "#A07B7C86",
			"headIndent": 36,
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"special-char": {
			"COMMENT": "Applies to * and -; deliberately using AvenirNext-Regular not AvenirNextCondensed-Regular otherwise **too** thin!",
			"__font": "Menlo-Regular",
			"font": "AvenirNext-Regular"
		},
		"quote-mark": {
			"color": "#35443250",
			"font": "noteplanstate",
			"headIndent": 36
		},
		"quote-content": {
			"color": "#434343",
			"type": "italic",
			"headIndent": 36,
			"lineSpacing": 0.90,
			"paragraphSpacing": 4,
			"paragraphSpacingBefore": 0
		},
		"markdown-link": {
			"color": "#C89240"
		},
		"markdown-link-url": {
			"color": "#465E68"
		},
		"link": {
			"color": "#C88E6B"
		},
		"time": {
			"color": "#E36868"
		},
		"schedule-to-date-link": {
			"color": "#E8AE9B"
		},
		"done-date": {
			"color": "#AAA",
			"size": 14
		},
		"schedule-from-date-link": {
			"color": "#E8AE9B"
		},
		"note-title-link": {
			"color": "#97B853"
		},
		"hashtag": {
			"color": "#5A64A2",
			"backgroundColor": "#10335522",
			"borderRadius": 4,
			"leftBorderPadding": -1,
			"rightBorderPadding": -1,
			"type": ""
		},
		"attag": {
			"color": "#5A64A2",
			"backgroundColor": "#10553322",
			"borderRadius": 4,
			"leftBorderPadding": -1,
			"rightBorderPadding": -1,
			"type": ""
		},
		"phonenumber": {
			"color": "#769EB3"
		},
		"field-key": {
			"COMMENT": "Simplified form of YAML field identification, limited to 25 chars in field name at start of line",
			"regex": "^([^\\h#:\\-\\+\\*][\\s\\w]{3,26})(:\\h+)(?=[\\w\\d@!#]+)",
			"matchPosition": 1,
			"color": "#5A64A2",
			"lineSpacing": 0.8,
			"paragraphSpacing": 0,
			"paragraphSpacingBefore": 0
		},
		"field-value": {
			"COMMENT": "now turned off",
			"_regex": "^([^:\\+\\*]{3,26}:)\\h+(.*)$",
			"_matchPosition": 2,
			"_color": "#BB5A64A2",
			"_lineSpacing": 0.8,
			"_paragraphSpacing": 0,
			"_paragraphSpacingBefore": 0
		},
		"attribute": {
			"regex": "^([^:]{3,})(::)",
			"color": "#5A64A2",
			"matchPosition": 1
		},
		"attribute-right-colons": {
			"regex": "^([^:]{3,})(::)",
			"color": "#5A64A2",
			"matchPosition": 2,
			"isMarkdownCharacter": true
		},
		"NoteLinks-main": {
			"regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 8,
			"color": "#C5487A"
		},
		"NoteLinks-left-brackets": {
			"regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 1,
			"color": " #C5487A",
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},
		"NoteLinks-right-brackets": {
			"regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 3,
			"color": " #C5487A",
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},
		"NoteLinks-main-done": {
			"regex": "(?:^|\\h)(x\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"isRevealOnCursorRange": true,
			"font": "noteplanstate",
			"color": "#97B853"
		},
		"NoteLinks-main-cancelled": {
			"regex": "(?:^|\\h)(-\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"isRevealOnCursorRange": true,
			"font": "noteplanstate",
			"color": "#C5487A"
		},
		"URL-link": {
			"regex": "\\[[^]]*?\\]\\(.*?\\)",
			"isRevealOnCursorRange": true,
			"comment": "Put cursor anywhere in markdown link and the []() syntax will be revealed. The following 4 do the hiding the rest of the time."
		},
		"URL-link-left-square-bracket": {
			"COMMENT": "Slightly more complex than the others to avoid problem with start-of-line task markers",
			"regex": "(\\[)([^]]*?)(\\])(\\()(.*?)(\\))",
			"matchPosition": 1,
			"isHiddenWithoutCursor": true,
			"isMarkdownCharacter": true
		},
		"URL-link-right-square-bracket": {
			"regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
			"matchPosition": 3,
			"isHiddenWithoutCursor": true,
			"isMarkdownCharacter": true
		},
		"URL-link-left-bracket": {
			"regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
			"matchPosition": 4,
			"isHiddenWithoutCursor": true,
			"isMarkdownCharacter": true
		},
		"URL-link-right-bracket": {
			"regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
			"matchPosition": 6,
			"isHiddenWithoutCursor": true,
			"isMarkdownCharacter": true
		},
		"strikethrough": {
			"__regex": "(~~)([^~]+)(~~)",
			"__matchPosition": 2,
			"strikethroughStyle": 2,
			"isRevealOnCursorRange": true
		},
		"strikethrough-left-tilde": {
			"__regex": "(~~)([^~]+)(~~)",
			"__matchPosition": 1,
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"color": "#d9d9d9",
			"isRevealOnCursorRange": true
		},
		"strikethrough-right-tilde": {
			"__regex": "(~~)([^~]+)(~~)",
			"__matchPosition": 3,
			"isMarkdownCharacter": true,
			"color": "#d9d9d9",
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},
		"deadline": {
			"regex": "\\h(!\\d{4}-\\d{2}-\\d{2})\\b",
			"matchPosition": 1,
			"font": "AvenirNextCondensed-Bold",
			"color": "#C53119"
		},
		"working-on": {
			"backgroundColor": "#22006600",
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"flagged-1": {
			"__regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{1}\\h.*)|(.*\\h+\\!{1}\\h.*)|(.*\\h\\!{1}))$",
			"__matchPosition": 2,
			"backgroundColor": "#FFE5E5",
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"flagged-2": {
			"__regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{2}\\h.*)|(.*\\h+\\!{2}\\h.*)|(.*\\h\\!{2}))$",
			"__matchPosition": 2,
			"backgroundColor": "#FFD5D5",
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"flagged-3": {
			"__regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{3}\\h.*)|(.*\\h+\\!{3}\\h.*)|(.*\\h\\!{3}))$",
			"__matchPosition": 2,
			"backgroundColor": "#FFC5C5",
			"lineSpacing": 0.90,
			"paragraphSpacing": 4
		},
		"starred": {
			"regex": "(^|\\v)(.*‚≠êÔ∏è.*)($|\\v)",
			"matchPosition": 2,
			"backgroundColor": "#FFD5D5"
		},
		"code": {
			"font": "Menlo-Regular",
			"color": "#4A54A2",
			"backgroundColor": "#F4F4FB",
			"size": 14
		},
		"code-left-backtick": {
			"color": "#884A54A2"
		},
		"code-right-backtick": {
			"color": "#884A54A2"
		},
		"code-fence": {
			"theme": "github",
			"body": {
				"font": "Menlo-Regular",
				"lineSpacing": 0.8,
				"paragraphSpacingBefore": 0,
				"paragraphSpacing": 0,
				"size": 14,
				"_backgroundColor": "#e9f9e9"
			},
			"corner-radius": 6,
			"fence-open": {
				"color": "#6584A2"
			},
			"fence-close": {
				"color": "#6584A2"
			},
			"language-specifier": {
				"font": "Menlo-Italic",
				"color": "#afafaf",
				"size": 12,
				"paragraphSpacingBefore": 0,
				"paragraphSpacing": 0
			}
		},
		"#waiting": {
			"regex": "^* (.*#waiting.*)$",
			"matchPosition": 1,
			"textColor": "#222E3388",
			"font": "AvenirNextCondensed-Italic"
		},
		"Templates_tags-opener": {
			"regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
			"matchPosition": 1,
			"color": "#BB3B3AB2",
			"font": "Menlo-Regular",
			"size": 15
		},
		"Templates_tags": {
			"regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
			"matchPosition": 2,
			"color": "#3B3AB2",
			"font": "Menlo-Regular",
			"size": 15
		},
		"Templates_tags-closer": {
			"regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
			"matchPosition": 3,
			"color": "#BB3B3AB2",
			"font": "Menlo-Regular",
			"size": 15
		},
		"Templates_tags-commented-out": {
			"regex": "(<%#\\s+.*?\\s+[\\-]?%>)",
			"matchPosition": 1,
			"color": "#773B3AB2",
			"font": "Menlo-Regular",
			"size": 15
		},
		"unfinished": {
			"regex": "\\?\\?\\?",
			"color": "#FFFFFF",
			"backgroundColor": "#FF2020"
		},
		"eventID": {
			"regex": "(event:[A-F0-9-]{36,37})",
			"matchPosition": 1,
			"color": "#CCA949",
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": false
		},
		"Timeblock-Hide-Tag": {
			"comment": "FIXME:",
			"regex": "\\s#üïë",
			"matchPosition": 0,
			"backgroundColor": "#FAFAFA",
			"isRevealOnCursorRange": true,
			"isHiddenWithoutCursor": true,
			"type": "nolink"
		},
		"end-of-line-comment-hide": {
			"regex": "(.*)( // )(.*)",
			"matchPosition": 3,
			"isHiddenWithoutCursor": true,
			"color": "#BB222E33"
		},
		"end-of-line-comment-show": {
			"regex": "(.* // .*)",
			"matchPosition": 1,
			"isRevealOnCursorRange": true
		},
		"end-of-line-comment-mark": {
			"regex": "(.*)( // )",
			"matchPosition": 2,
			"isMarkdownCharacter": true,
			"color": "#99818475"
		},
		"inline-comment": {
			"regex": "(%)(%([^%]+%?)+%)(%)",
			"matchPosition": 2,
			"isRevealOnCursorRange": true,
			"isHiddenWithoutCursor": true,
			"color": "#BB222E33"
		},
		"inline-comment-mark-start": {
			"regex": "[\\s^](%%)[^\\s]",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"color": "#99818475"
		},
		"inline-comment-mark-end": {
			"regex": "\\b(%%)[\\W^]",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"color": "#99818475"
		},
		"arrow-link": {
			"regex": "[\\s^]>([^>]*?)<[$\\s\\b\\.\\)\\]\\}]",
			"matchPosition": 1,
			"urlPosition": 1,
			"backgroundColor": "#10000000",
			"isRevealOnCursorRange": true,
			"type": "noteLink",
			"prefix": "noteplan://x-callback-url/openNote?noteTitle="
		},
		"arrow-left-delimiter": {
			"regex": "(>)(\\S[^<]*)(<)",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},
		"arrow-right-delimiter": {
			"regex": "(>)(\\S[^<]*)(<)",
			"matchPosition": 3,
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},
		"verse-indicator": {
			"regex": "(¬ß)(.*)",
			"matchPosition": 1,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true,
			"isMarkdownCharacter": true,
			"color": "#99818475"
		},
		"verse-text": {
			"regex": "(¬∂)(.*)",
			"matchPosition": 2,
			"color": "#F04060",
			"backgroundColor": "#f0f0f0",
			"borderRadius": 4
		}
	}
}